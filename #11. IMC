public class MinhaClass {
    public static void main(String[] args) {

        //0 -> peso div (altura * altura)
        //1 -> Pegar o peso;
        //2 -> Pegar a altura;

        float peso = 75;
        float altura = 1.78f;

        float resultado = peso / (altura * altura);
        System.out.println(
                "O IMC para o peso é " + peso + " e altura "
                        + altura + " é " + resultado);

    }
}
